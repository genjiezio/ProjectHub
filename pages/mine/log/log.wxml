<!--miniprogram/pages/home/log/log.wxml-->
<view class="weui-cells weui-cells_after-title">
    <view class="weui-cell weui-cell_active">
        <view class="weui-cell__hd">
            <view class="weui-label">账号</view>
        </view>
        <view class="weui-cell__bd">
            <input class="weui-input" bindinput='accountInput' placeholder="请输入账号"/>
        </view>
    </view>
    <view class="weui-cell weui-cell_active">
        <view class="weui-cell__hd">
            <view class="weui-label">密码</view>
        </view>
        <view class="weui-cell__bd">
            <input class="weui-input" bindinput='passwordInput' name="passWord" type="{{typeName}}" placeholder='请输入密码'></input>
        </view>
    </view>
    
</view>
<text>\n\n</text>
<button  bindtap="log_user" open-type="getUserInfo">登录</button>

